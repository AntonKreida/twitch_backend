generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  firstName   String
  lastName    String
  username    String   @unique
  displayName String
  email       String   @unique
  createAt    DateTime @default(now())
  updateAt    DateTime @updatedAt
}
